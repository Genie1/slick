import scala.slick.typeProviders._
object Db1 extends TypeProvider.Db("coffees")
import Db1.driver.simple._
import Database.threadLocalSession
Db1.database.withSession {
   Query(Db1.Coffees) foreach println
}
object Db2 extends TypeProvider.Db("supp")
import Db2.driver.simple._
import Database.threadLocalSession
Db2.database.withSession {
   Query(Db2.Suppliers) foreach println
}
